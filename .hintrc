{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "marquee": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[allow]",
          "iframe[loading]"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ]
  }
}